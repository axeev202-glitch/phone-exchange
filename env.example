# Конфигурация для VDS
# Скопируйте этот файл в .env и заполните значения

# Порт сервера (обычно 3000 для Node.js)
PORT=3000

# Хост (0.0.0.0 для прослушивания всех интерфейсов)
HOST=0.0.0.0

# Домен вашего сайта (опционально, для логирования)
DOMAIN=your-domain.com

# Окружение
NODE_ENV=production

